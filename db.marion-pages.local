;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	debian.marion-pages.local. root.marion-pages.local. (
			011019		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	smtp.marion-pages.local.
@	IN	NS	pop.marion-pages.local.
@	IN	NS	imap.marion-pages.local.

